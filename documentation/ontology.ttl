#################################################################
#    Object Properties
#################################################################

###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#definedByEnvelop
:definedByEnvelop rdf:type owl:ObjectProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain :ThermalNeed ;
                  rdfs:range :ThermalComfortEnvelop .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#definedByRange
:definedByRange rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:range :ParameterRange .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#detailedBy
:detailedBy rdf:type owl:ObjectProperty ,
                     owl:FunctionalProperty .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#drivenBy
:drivenBy rdf:type owl:ObjectProperty .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#hasBehavior
:hasBehavior rdf:type owl:ObjectProperty ;
             rdfs:domain :Occupant ;
             rdfs:range :Behavior .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#hasBuildingType
:hasBuildingType rdf:type owl:ObjectProperty .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#hasCoefficient
:hasCoefficient rdf:type owl:ObjectProperty ;
                rdfs:domain :InteractionFormula .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#hasDayOfWeek
:hasDayOfWeek rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :DriverTime ;
              rdfs:range <http://www.w3.org/2006/time#DayOfWeek> .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#hasEnvironmentParameter
:hasEnvironmentParameter rdf:type owl:ObjectProperty ;
                         rdfs:domain :Environment ;
                         rdfs:range :EnvironmentParameter .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#hasIndependentVariable
:hasIndependentVariable rdf:type owl:ObjectProperty ;
                        rdfs:domain :InteractionFormula .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#hasMeeting
:hasMeeting rdf:type owl:ObjectProperty ;
            rdfs:domain <https://bimerr.iot.linkeddata.es/def/building#Space> ;
            rdfs:range :Meeting .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#hasOccupant
:hasOccupant rdf:type owl:ObjectProperty ;
             rdfs:domain <https://bimerr.iot.linkeddata.es/def/building#Space> ;
             rdfs:range :Occupant .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#hasSeason
:hasSeason rdf:type owl:ObjectProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :DriverTime ;
           rdfs:range :Season .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#hasSpace
:hasSpace rdf:type owl:ObjectProperty ;
          rdfs:domain <https://bimerr.iot.linkeddata.es/def/building#Building> ;
          rdfs:range <https://bimerr.iot.linkeddata.es/def/building#Space> .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#hasSpaceType
:hasSpaceType rdf:type owl:ObjectProperty .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#hasSystem
:hasSystem rdf:type owl:ObjectProperty ;
           rdfs:domain <https://bimerr.iot.linkeddata.es/def/building#Space> ;
           rdfs:range :System .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#hasTimeOfDay
:hasTimeOfDay rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :DriverTime ;
              rdfs:range :TimeOfDay .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#influencedByNeed
:influencedByNeed rdf:type owl:ObjectProperty ;
                  rdfs:range :Need .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#interactsWith
:interactsWith rdf:type owl:ObjectProperty .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#leadsTo
:leadsTo rdf:type owl:ObjectProperty .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#modeledBy
:modeledBy rdf:type owl:ObjectProperty ,
                    owl:FunctionalProperty .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#operationMode
:operationMode rdf:type owl:ObjectProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain :System .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#programmedAt
:programmedAt rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ;
              rdfs:range :Schedule .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#relatedTo
:relatedTo rdf:type owl:ObjectProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :ParameterRange ;
           rdfs:range :EnvironmentParameter .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#specifiedBy
:specifiedBy rdf:type owl:ObjectProperty ,
                      owl:FunctionalProperty .


