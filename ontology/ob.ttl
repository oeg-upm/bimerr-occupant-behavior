#################################################################
#    Object Properties
#################################################################

###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#definedByEnvelop
op:definedByEnvelop rdf:type owl:ObjectProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain op:ThermalNeed ;
                    rdfs:range op:ThermalComfortEnvelop .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#definedByRange
op:definedByRange rdf:type owl:ObjectProperty ,
                           owl:FunctionalProperty ;
                  rdfs:range op:ParameterRange .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#detailedBy
op:detailedBy rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#drivenBy
op:drivenBy rdf:type owl:ObjectProperty .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#hasBehavior
op:hasBehavior rdf:type owl:ObjectProperty ;
               rdfs:domain op:Occupant ;
               rdfs:range op:Behavior .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#hasBuildingType
op:hasBuildingType rdf:type owl:ObjectProperty .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#hasCoefficient
op:hasCoefficient rdf:type owl:ObjectProperty ;
                  rdfs:domain op:InteractionFormula .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#hasDayOfWeek
op:hasDayOfWeek rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain op:DriverTime ;
                rdfs:range time:DayOfWeek .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#hasEnvironmentParameter
op:hasEnvironmentParameter rdf:type owl:ObjectProperty ;
                           rdfs:domain op:Environment ;
                           rdfs:range op:EnvironmentParameter .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#hasIndependentVariable
op:hasIndependentVariable rdf:type owl:ObjectProperty ;
                          rdfs:domain op:InteractionFormula .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#hasMeeting
op:hasMeeting rdf:type owl:ObjectProperty ;
              rdfs:domain building:Space ;
              rdfs:range op:Meeting .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#hasOccupant
op:hasOccupant rdf:type owl:ObjectProperty ;
               rdfs:domain building:Space ;
               rdfs:range op:Occupant .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#hasSeason
op:hasSeason rdf:type owl:ObjectProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain op:DriverTime ;
             rdfs:range op:Season .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#hasSpace
op:hasSpace rdf:type owl:ObjectProperty ;
            rdfs:domain building:Building ;
            rdfs:range building:Space .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#hasSpaceType
op:hasSpaceType rdf:type owl:ObjectProperty .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#hasSystem
op:hasSystem rdf:type owl:ObjectProperty ;
             rdfs:domain building:Space ;
             rdfs:range op:System .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#hasTimeOfDay
op:hasTimeOfDay rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain op:DriverTime ;
                rdfs:range op:TimeOfDay .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#influencedByNeed
op:influencedByNeed rdf:type owl:ObjectProperty ;
                    rdfs:range op:Need .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#interactsWith
op:interactsWith rdf:type owl:ObjectProperty .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#leadsTo
op:leadsTo rdf:type owl:ObjectProperty .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#modeledBy
op:modeledBy rdf:type owl:ObjectProperty ,
                      owl:FunctionalProperty .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#operationMode
op:operationMode rdf:type owl:ObjectProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain op:System .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#programmedAt
op:programmedAt rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:range op:Schedule .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#relatedTo
op:relatedTo rdf:type owl:ObjectProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain op:ParameterRange ;
             rdfs:range op:EnvironmentParameter .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#specifiedBy
op:specifiedBy rdf:type owl:ObjectProperty ,
                        owl:FunctionalProperty .


#################################################################
#    Data properties
#################################################################

###  http://www.w3.org/2002/07/owl#topDataProperty
owl:topDataProperty rdfs:range xsd:float .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#buildingAddress
op:buildingAddress rdf:type owl:DatatypeProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain building:Building ;
                   rdfs:range xsd:string .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#coefficientName
op:coefficientName rdf:type owl:DatatypeProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain op:Coefficient ;
                   rdfs:range xsd:string .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#coefficientValue
op:coefficientValue rdf:type owl:DatatypeProperty ;
                    rdfs:domain op:Coefficient ;
                    rdfs:range xsd:float .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#controlValue
op:controlValue rdf:type owl:DatatypeProperty .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#description
op:description rdf:type owl:DatatypeProperty ;
               rdfs:range xsd:string .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#duration
op:duration rdf:type owl:DatatypeProperty ,
                     owl:FunctionalProperty ;
            rdfs:range xsd:integer .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#endTime
op:endTime rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain op:Schedule ;
           rdfs:range xsd:dateTime .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#identifier
op:identifier rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#maxNumberOccupants
op:maxNumberOccupants rdf:type owl:DatatypeProperty ,
                               owl:FunctionalProperty ;
                      rdfs:domain building:Space ;
                      rdfs:range xsd:integer .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#maxRangeValue
op:maxRangeValue rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain op:ParameterRange ;
                 rdfs:range xsd:float .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#meetingProbability
op:meetingProbability rdf:type owl:DatatypeProperty ,
                               owl:FunctionalProperty ;
                      rdfs:domain op:Meeting ;
                      rdfs:range xsd:float .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#minNumberOccupants
op:minNumberOccupants rdf:type owl:DatatypeProperty ,
                               owl:FunctionalProperty ;
                      rdfs:domain building:Space ;
                      rdfs:range xsd:integer .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#minRangeValue
op:minRangeValue rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain op:ParameterRange ;
                 rdfs:range xsd:float .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#occupantAge
op:occupantAge rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain op:Occupant ;
               rdfs:range xsd:integer .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#occupantGender
op:occupantGender rdf:type owl:DatatypeProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain op:Occupant ;
                  rdfs:range xsd:string .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#occupantJobtype
op:occupantJobtype rdf:type owl:DatatypeProperty ;
                   rdfs:domain op:Occupant ;
                   rdfs:range xsd:string .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#occupantLifestyle
op:occupantLifestyle rdf:type owl:DatatypeProperty ;
                     rdfs:domain op:Occupant ;
                     rdfs:range xsd:string .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#occupantName
op:occupantName rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain op:Occupant ;
                rdfs:range xsd:string .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#parameterName
op:parameterName rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain op:EnvironmentParameter ;
                 rdfs:range xsd:string .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#parameterUnit
op:parameterUnit rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain op:EnvironmentParameter ;
                 rdfs:range xsd:string .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#percentTimePresence
op:percentTimePresence rdf:type owl:DatatypeProperty ;
                       rdfs:domain op:SpaceOccupancySpecification ;
                       rdfs:range xsd:float .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#startTime
op:startTime rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain op:Schedule ;
             rdfs:range xsd:dateTime .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#typicalTime
op:typicalTime rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain op:EventSpecification .


#################################################################
#    Classes
#################################################################

###  http://www.w3.org/2006/time#DayOfWeek
time:DayOfWeek rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Class ;
                                     owl:oneOf ( time:Friday
                                                 time:Monday
                                                 time:Saturday
                                                 time:Sunday
                                                 time:Thursday
                                                 time:Tuesday
                                                 time:Wednesday
                                               )
                                   ] ;
               rdfs:label "DayOfWeek"@en .


###  https://bimerr.iot.linkeddata.es/def/building#Building
building:Building rdf:type owl:Class ;
                  rdfs:subClassOf [ rdf:type owl:Restriction ;
                                    owl:onProperty op:hasBuildingType ;
                                    owl:someValuesFrom building:BuildingType
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty op:hasSpace ;
                                    owl:someValuesFrom building:Space
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty op:hasSpace ;
                                    owl:allValuesFrom building:Space
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty op:identifier ;
                                    owl:someValuesFrom xsd:string
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty op:buildingAddress ;
                                    owl:allValuesFrom xsd:string
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty op:description ;
                                    owl:allValuesFrom xsd:string
                                  ] ;
                  rdfs:comment "Building where the occupancy behaviors occur."@en ;
                  rdfs:label "Building"@en .


###  https://bimerr.iot.linkeddata.es/def/building#BuildingType
building:BuildingType rdf:type owl:Class .


###  https://bimerr.iot.linkeddata.es/def/building#Space
building:Space rdf:type owl:Class ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty op:hasSpaceType ;
                                 owl:someValuesFrom op:SpaceType
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty op:hasMeeting ;
                                 owl:allValuesFrom op:Meeting
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty op:hasOccupant ;
                                 owl:allValuesFrom op:Occupant
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty op:hasSystem ;
                                 owl:allValuesFrom op:System
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty op:identifier ;
                                 owl:someValuesFrom xsd:string
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty op:description ;
                                 owl:allValuesFrom xsd:string
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty op:maxNumberOccupants ;
                                 owl:allValuesFrom xsd:integer
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty op:minNumberOccupants ;
                                 owl:allValuesFrom xsd:integer
                               ] ;
               rdfs:comment "Class to represent the different spaces the building have and where the occupant behaviors occur." ;
               rdfs:label "Space"@en .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#AccousticNeed
op:AccousticNeed rdf:type owl:Class ;
                 rdfs:subClassOf op:PhysicalNeed ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty op:definedByRange ;
                                   owl:someValuesFrom op:ParameterRange
                                 ] ;
                 rdfs:label "Accoustic Need"@en .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#Action
op:Action rdf:type owl:Class ;
          rdfs:comment "Actions are the interactions with systems or activities that occupants can perform to achieve environmental comfort."@en ;
          rdfs:label "Action"@en .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#Behavior
op:Behavior rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty op:leadsTo ;
                              owl:someValuesFrom op:Action
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty op:drivenBy ;
                              owl:allValuesFrom op:Drivers
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty op:influencedByNeed ;
                              owl:allValuesFrom op:Need
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty op:interactsWith ;
                              owl:allValuesFrom op:System
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty op:identifier ;
                              owl:someValuesFrom xsd:string
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty op:description ;
                              owl:allValuesFrom xsd:string
                            ] ;
            rdfs:comment "Class that represent the behaviour of the occupant."@en ;
            rdfs:label "Behavior"@en .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#Coefficient
op:Coefficient rdf:type owl:Class ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty op:coefficientName ;
                                 owl:someValuesFrom xsd:string
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty op:coefficientValue ;
                                 owl:someValuesFrom xsd:float
                               ] ;
               rdfs:comment "N-ary class to represent the value and the names of coefficients inside mathematical formulas."@en ;
               rdfs:label "Coefficient"@en .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#ConstantValueFormula
op:ConstantValueFormula rdf:type owl:Class ;
                        rdfs:subClassOf op:InteractionFormula ;
                        rdfs:comment "Class to represent mathematical formulas of the type Constant Value."@en ;
                        rdfs:label "Constant Value Formula"@en .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#ControlType
op:ControlType rdf:type owl:Class .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#DaylightFactorParameter
op:DaylightFactorParameter rdf:type owl:Class ;
                           rdfs:subClassOf op:EnvironmentParameter ;
                           rdfs:comment "Parameter to represent the indoor daylight."@en ;
                           rdfs:label "Daylight Factor Parameter"@en .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#DriverTime
op:DriverTime rdf:type owl:Class ;
              rdfs:subClassOf op:Drivers ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty op:hasDayOfWeek ;
                                owl:allValuesFrom time:DayOfWeek
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty op:hasSeason ;
                                owl:allValuesFrom op:Season
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty op:hasTimeOfDay ;
                                owl:allValuesFrom op:TimeOfDay
                              ] ;
              rdfs:comment "N-ary class to define the time, day and season when a specific driver occurs."@en ;
              rdfs:label "Driver Time"@en .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#Drivers
op:Drivers rdf:type owl:Class ;
           rdfs:comment "Drivers represent the environmental factors that stimulate occupants to fulfill a physical, physiological or psychological need."@en ;
           rdfs:label "Drivers"@en .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#Environment
op:Environment rdf:type owl:Class ;
               rdfs:subClassOf op:Drivers ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty op:hasEnvironmentParameter ;
                                 owl:someValuesFrom op:EnvironmentParameter
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty op:hasEnvironmentParameter ;
                                 owl:allValuesFrom op:EnvironmentParameter
                               ] ;
               rdfs:comment "Class to represent the environmental physical conditions that could be causing the occupant behavior."@en ;
               rdfs:label "Environment"@en .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#EnvironmentParameter
op:EnvironmentParameter rdf:type owl:Class ;
                        rdfs:subClassOf [ rdf:type owl:Restriction ;
                                          owl:onProperty op:identifier ;
                                          owl:someValuesFrom xsd:string
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty op:parameterUnit ;
                                          owl:someValuesFrom xsd:string
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty op:description ;
                                          owl:allValuesFrom xsd:string
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty op:parameterName ;
                                          owl:allValuesFrom xsd:string
                                        ] ;
                        rdfs:comment "Class that represent a physical variable of the environment where the driver occurs. It could represent variables that impact directly on the comfort levels of the occupant."@en ;
                        rdfs:label "Environment Parameter"@en .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#Equipment
op:Equipment rdf:type owl:Class ;
             rdfs:subClassOf op:System ;
             rdfs:comment "Class to represent equipments the occupant interact with but do not influence the environment. These systems also contribute in the energy performance analysis." ;
             rdfs:label "Equipment"@en .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#Event
op:Event rdf:type owl:Class ;
         rdfs:subClassOf op:Drivers ;
         rdfs:label "Event"@en .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#EventSpecification
op:EventSpecification rdf:type owl:Class ;
                      rdfs:subClassOf op:MovementSpecification ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty op:detailedBy ;
                                        owl:someValuesFrom op:Event
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty op:programmedAt ;
                                        owl:someValuesFrom op:Schedule
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty op:typicalTime ;
                                        owl:someValuesFrom xsd:dateTime
                                      ] ;
                      owl:disjointWith op:SpaceOccupancySpecification ;
                      rdfs:label "Event Specification"@en .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#GlareParameter
op:GlareParameter rdf:type owl:Class ;
                  rdfs:subClassOf op:EnvironmentParameter ;
                  rdfs:comment "Parameter to represent the glare inside a space."@en ;
                  rdfs:label "Glare Parameter"@en .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#HVAC
op:HVAC rdf:type owl:Class ;
        rdfs:subClassOf op:System ;
        rdfs:label "HVAC"@en .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#Habit
op:Habit rdf:type owl:Class ;
         rdfs:subClassOf op:Drivers ;
         rdfs:comment "Class to represent occupant habits."@en ;
         rdfs:label "Habit"@en .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#IAQNeed
op:IAQNeed rdf:type owl:Class ;
           rdfs:subClassOf op:PhysicalNeed ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty op:definedByRange ;
                             owl:someValuesFrom op:ParameterRange
                           ] ;
           rdfs:label "IAQ Need"@en .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#IAQParameter
op:IAQParameter rdf:type owl:Class ;
                rdfs:subClassOf op:EnvironmentParameter ;
                rdfs:comment "Parameter to represent the indoor air quality."@en ;
                rdfs:label "IAQ Parameter"@en .


###  https://bimerr.iot.linkeddata.es/def/occupancy-profile#IlluminanceParameter
